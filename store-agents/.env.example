# Store Agents Environment Configuration Template
# Copy this file to .env and update the paths and values for your environment

# Google Generative AI Configuration
GOOGLE_GENAI_USE_VERTEXAI=0
GOOGLE_API_KEY=your_google_api_key_here

# Google Cloud Vision API Configuration
# Set this to the path of your Google Cloud service account JSON file
GOOGLE_APPLICATION_CREDENTIALS=./vision-api-service.json

# Firebase Configuration
# Set this to the path of your Firebase service account key JSON file
FIREBASE_SERVICE_ACCOUNT_KEY=./firebase-service-account-key.json

# Optional: Set your Firebase project ID if needed
FIREBASE_PROJECT_ID=your_firebase_project_id

# Server Configuration
STORE_AGENTS_HOST=127.0.0.1
STORE_AGENTS_PORT=8003
